/*
 * Tests for Integers 1
 * Show notation
 */
package tests.syntax.valid {
	
	affine integer1a [N]->{ : }
		outputs
			X : {}
		let
			X = -1;
	.
	
	affine integer1b [N]->{ : }
		outputs
			X : {}
		let
			X = -1--1;
	.
	
	affine integer1c [N]->{ : }
		outputs
			X : {}
		let
			X = -1*-1;
	.
	
	affine integer1d [N]->{ : }
		outputs
			X : {}
		let
			X = 1-1;
	.
	
	
	affine integer1e [N]->{ : }
		outputs
			X : {}
		let
			X = val(->-1);
	.
	
	affine integer1f [N]->{ : }
		outputs
			X : [N]
		let
			X = (i->)@-1;
	.

}